package com.linkedin.algos;

public class PalindromeLL {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
        Node<String> first = new Node<String>("a");
        Node<String> second = new Node<String>("b");
        Node<String> third = new Node<String>("c");
        Node<String> fourth = new Node<String>("b");
        Node<String> fifth = new Node<String>("a");

        first.setNext(second);
        second.setNext(third);
        third.setNext(fourth);
        fourth.setNext(fifth);
        fifth.setNext(third);
        Node.printLL(first);
//
//      Node.printLL(fourth);
        new FindLoopLL<String>().findLoop(first);
}

}
