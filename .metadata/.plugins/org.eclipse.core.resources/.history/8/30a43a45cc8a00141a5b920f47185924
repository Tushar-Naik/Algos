package com.flipkart.programs;

public class SubStringSearch {

	/**
	 * @param args
	 */
	public static final String SEARCH = "ABACADABRCAC";
	public static final String PATTERN = "ABRA";
	
	public static void search(){
		for(int i = 0;i<SEARCH.length()-1;i++){
			for(int j =0;j<PATTERN.length()-1 ; j++){
				if(SEARCH.charAt(j+i)==PATTERN.charAt(j)){
					System.out.println((j+i)+"matches"+j);
				}else{
					break;
				}
				if(j==PATTERN.length()-2) System.out.println("Matched");
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		search();

	}

}
