package com.flipkart.programs;

public class SubStringSearch {

	/**
	 * @param args
	 */
	public static final String SEARCH = "ABACADABRAC";
	public static final String PATTERN = "ABRA";
	
	//O(M*N) can be O(N^2)
	public static void search(){
		int n = SEARCH.length();
		int m = PATTERN.length();
		for(int i = 0;i<n-m;i++){
			for(int j =0;j<m ; j++){
				System.out.println(SEARCH.charAt(j+i) +" "+PATTERN.charAt(j));
				if(SEARCH.charAt(j+i)!=PATTERN.charAt(j)){
					System.out.println("didnt match");break;
				}
				if(j==PATTERN.length()-1) {System.out.println("Matched "+i+" to "+ (i+PATTERN.length()-1)); return;}
			}
		}
	}
	
	public static void searchBoyerMayer(){
		//Precompute a right array based on pattern length
		
		int [] right = new int[PATTERN.length()];
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		search();

	}

}
