package com.flipkart.programs;

public class MyHashMap<Key,Value> {
	private final Node<Value>[] array;
	private static final int CAPACITY = 10;
	
	public MyHashMap(){
		array = new Node[CAPACITY];
	}
	public void put (Key key, Value val){
		int hash = key.hashCode() & 0x7fffffff  % CAPACITY;
		for(Node<Value> n = array[hash];n!=null;n = n.next){
			n=n.next;
		}
		array[hash] = new Node<Value>(val,array[hash]);
		
	}
	public Value get (Key key){

		return null;
	}
	public static void main(String[] args) {
		MyHashMap<Integer,String> map = new MyHashMap<Integer, String>();
		map.put(40,"AUM");
		map.put(50, "ONE");
	}
}
  class Node<Value>{
		Node<Value> next;
		Value val;
		public Node(Value val, Node<Value> next){
			this.val = val;
			this.next = next;
		}
	}

