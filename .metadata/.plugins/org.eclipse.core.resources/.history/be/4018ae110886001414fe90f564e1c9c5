package com.flipkart.programs;

import java.util.HashMap;

public class LRUCache<K,V> {

	/**
	 * Linkedlist + Hashmap
	 */
	private Node head ;
	private HashMap<K, V> map = new HashMap<K,V>();
	private static final int CAPACTITY = 10;
	private int currentSize = 0;
	
	private static class Node<K,V>{
		Node next;
		Node prev;
		V val;
		public Node(Node prev, Node next, V val) {
			this.prev = prev;
			this.next = next;
			this.val = val;
		}
	}
	
	public void put(K key, V val){
		map.put(key, val);
		addToLinkedList(val);
		
	}
	
	private void addToLinkedList(V val) {
		if (head==null){
			Node<K,V> newNode = new Node<K, V>(null, null ,val) ;
			head = newNode;
			currentSize++;
		}else {
			//Capacity check
			Node tmp = head;
			Node<K,V> newNode = new Node<K, V>(head, null ,val) ;
			head.next = newNode;
			head = newNode;
		}
	}

	public V get(K key){
		return null;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
